<h1>Listing tag_categories</h1>

<table>
  <tr>
    <th>Name</th>
  </tr>

<% @tag_categories.each do |tag_category| %>
  <tr>
    <td><%=h tag_category.name %></td>
    <td><%= link_to 'Show', tag_category %></td>
    <td><%= link_to 'Edit', edit_tag_category_path(tag_category) %></td>
    <td><%= link_to 'Destroy', tag_category, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New tag_category', new_tag_category_path %>