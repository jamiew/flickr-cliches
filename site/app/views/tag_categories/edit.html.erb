<h1>Editing tag_category</h1>

<% form_for(@tag_category) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @tag_category %> |
<%= link_to 'Back', tag_categories_path %>